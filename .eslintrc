{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "extends": ["eslint:recommended"],
    "parser": "@babel/eslint-parser",
    "rules": {
        "semi": "error",
        "no-multiple-empty-lines": "error",
        "space-in-parens": "error",
        "prefer-const": "error",
        "no-use-before-define": "error",
        "no-var": "error"
    }
}
